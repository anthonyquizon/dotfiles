[alias]
	ch = checkout
	b = branch
	s = status
	cm = commit -m
	unstage = reset HEAD --
	ai = add -i
	pu = push --set-upstream origin
	c = commit
	cht = checkout --track
	ca = commit --amend
	cam = !git add -A && git commit -m
	cad = add -A
	aA = add -A
[push]
	default = simple
[core]
	excludesfile = ~/.gitignore
